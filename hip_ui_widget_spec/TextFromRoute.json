{
  "type": "TextFromRoute",
  "mappings": [
    "@Text",
    {
      "type": "!figma-text-from-route"
    }
  ],
  "userData": {
    "key": {
      "type": "string",
      "default": null,
      "description": "They key, in the route's data object, from which to get the text to display"
    },
    "selectable": {
      "type": "boolean",
      "default": false,
      "description": "is this text selectable"
    }
  }
}